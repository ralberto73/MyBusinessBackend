
@{
    ViewData["Title"] = "View";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@model IEnumerable<MyBusiness.Models.Brand>



<div class="border backgroudWhite">
    <div class="row">
        <div class="col-6 ">
            <h2 class="text-primary">  &nbsp; Brand List </h2>
        </div>
        <div class="col-6 text-right">
            <a asp-action="Upset" class="btn btn-primary"><i class="fad fa-plus"></i> Create New Brand </a>
        </div>
    </div>
</div>


<table id="tblData" class="table" style="width:100%">
    <thead>
        <tr>
            <th>Name</th>
            <th>Created By</th>
            <th>Creation Date</th>
            <th>Update By</th>
            <th>Last Update </th>
            <th></th>
        </tr>
    </thead>

</table>

@section Scripts{
    <script src="~/js/brand-list.js"></script>
    // https://github.com/bhrugen/Uplift
}




